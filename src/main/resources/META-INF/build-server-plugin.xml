<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"
       default-autowire="constructor">

    <bean class="jetbrains.buildServer.auth.oauth.LoginViaOAuthLoginPageExtension" />
    <bean class="jetbrains.buildServer.auth.oauth.OAuthAuthenticationScheme" />
    <bean class="jetbrains.buildServer.auth.oauth.OAuthClient" />
    <bean class="jetbrains.buildServer.auth.oauth.ServerPrincipalFactory" />
    <bean class="jetbrains.buildServer.auth.oauth.LoginViaOAuthController" />
    <bean class="jetbrains.buildServer.auth.oauth.AuthenticationSchemeProperties" />
</beans>